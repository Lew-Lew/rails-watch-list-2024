<div class="modal fade" id="newListModal" tabindex="-1" aria-labelledby="newListModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title fs-5" id="newListModalLabel">Add a list</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <%= simple_form_for list do |f| %>
          <%= f.input :name %>
          <%= f.input :photo, as: :file %>
          <%= f.button :submit, 'add a list', class: 'btn btn-primary' %>
        <% end %>
      </div>
    </div>
  </div>
</div>